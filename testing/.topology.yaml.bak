name: "testing-topology"
prefix: ""
topology:
  kinds:
    ceos:
      image: "ceos:4.34.1F"
  nodes:
    leaf1:
      kind: "ceos"
      startup-config: "configs/leaf1.cfg"
      binds:
        - ../scripts:/mnt/scripts:rw
    leaf2:
      kind: "ceos"
      binds:
        - ../scripts:/mnt/scripts:rw
    leaf3:
      kind: "ceos"
      binds:
        - ../scripts:/mnt/scripts:rw
    spine1:
      kind: "ceos"
      binds:
        - ../scripts:/mnt/scripts:rw
